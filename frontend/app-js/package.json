{
  "name": "dart-ui-js",
  "version": "1.0.0",
  "description": "Javascript-only portion of dart-ui (Corpex and Forklift)",
  "main": "dist/jsAppExport.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist && rm -rf dart-ui-js-*.tgz",
    "init": "mkdir dist",
    "pre-build": "npm run clean && npm run init",
    "lint": "npx eslint --ignore-pattern *test*  src",
    "build": "npm run lint && npx babel src -d dist --presets=@babel/preset-env,@babel/preset-react --plugins=@babel/plugin-proposal-class-properties,syntax-dynamic-import --source-maps",
    "package": "npm run pre-build && npm run build && npm pack"
  },
  "repository": {
    "type": "git"
  },
  "author": "John Hungerford",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "dart-ui-scala13-components": "file:../scala13-components/dart-ui-scala13-components-1.0.0.tgz",
    "keycloak-js": "^17.0.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-beautiful-dnd": "13.0.0",
    "react-dom": "^17.0.2",
    "react-outside-click-handler": "^1.3.0",
    "react-pdf": "^4.2.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^6.2.1",
    "react-virtualized": "^9.22.3",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "typeface-roboto": "0.0.75"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.12.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "concat-with-sourcemaps": "1.0.7",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.4",
    "html-webpack-plugin": "4.5.2",
    "module-to-cdn": "3.1.5",
    "source-map-loader": "0.2.3",
    "webpack": "^4.28.4",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "4.7.4",
    "worker-loader": "^3.0.5"
  }
